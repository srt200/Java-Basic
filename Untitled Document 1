import.java.util.Scanner;
public class RollDie
{
	public static void main(String []args)
	{	int a=0,b=0,c=0,d=0,e=0,f=0;
		int d=(int)(Math.random()*6)+1;
		
		switch(d)
		{
			case 1:
			a++;
			break;
			case 2:
			b++;
			break;
			case 3:
			c++;
			break;
			case 4:
			d++;
			break;
			case 5:
			e++;
			break;
			case 6:
			f++;
			break;
		}
		
	}


}
